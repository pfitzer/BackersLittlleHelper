{
  "$schema": "https://schema.tauri.app/config/v2/capability",
  "identifier": "default",
  "description": "Default permissions for the application",
  "windows": ["*"],
  "permissions": [
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "fs:default",
    "fs:allow-app-read",
    "fs:allow-app-write",
    "fs:allow-app-read-recursive",
    "fs:allow-app-write-recursive",
    "core:path:default"
  ]
}