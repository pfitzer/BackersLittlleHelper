{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "productName": "BackersLittleHelper",
  "version": "0.1.1",
  "identifier": "com.backerslittlehelper.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5175",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "plugins": {},
  "app": {
    "windows": [
      {
        "title": "Backers Little Helper",
        "width": 1200,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}